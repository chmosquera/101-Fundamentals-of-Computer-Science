import math
import unittest

def f(x):
    return (7 * x**2) + (2 * x)
    
def g(x,y):
    return (x**2 + y**2) / (3 * x)

def hypotenuse(sideA,sideB):
    return math.sqrt(sideA**2 + sideB**2)

print hypotenuse(2.0,2.0)
print g(3,2)

def is_positive(number):
    return number > 0


print is_positive(1)
